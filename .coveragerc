[run]
source = webhook_server,tool_manager,letta_tool_utils
omit =
    */tests/*
    */test_*.py
    */__pycache__/*
    */venv/*
    */venvsource/*
    */archive/*
    run_server.py
    arxiv_integration.py

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    pass

[html]
directory = htmlcov

[xml]
output = coverage.xml
